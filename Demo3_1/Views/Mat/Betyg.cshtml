@model List<SelectListItem>


@{
    ViewData["Title"] = "Betyg";
}

<h2>Sätt betyg</h2>
<p>Hur smakade maten egentligen?</p>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Slutsats" method="post">
            <div class="form-group">
                @foreach (SelectListItem item in Model)
                {
                    <input type="radio" name="Betyg" value="@Html.DisplayFor(model => item.Value)">@item.Text <br />

                }

            </div>
            <div class="form-group">
                <input type="submit" value="Slutsats" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Börja om</a>
</div>

