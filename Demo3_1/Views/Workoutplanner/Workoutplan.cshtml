@model Laboration1.Models.Workoutplanner

@{
    ViewData["Title"] = "Workoutplan";
}

<h2>Workoutplan</h2>

<div>
    <h4>@ViewBag.text</h4>
    <hr />


    <dl class="row">
        <dt class="col-sm-4">  @Html.DisplayNameFor(model => model.Distance)</dt>
        <dd class="col-sm-8">  @Html.DisplayFor(model => model.Distance)</dd>
        <dt class="col-sm-4">  @Html.DisplayNameFor(model => model.Tempo)</dt>
        <dd class="col-sm-8">  @Html.DisplayFor(model => model.Tempo)</dd>
        <dt class="col-sm-4">  @Html.DisplayNameFor(model => model.Warmup)</dt>
        <dd class="col-sm-8">  @Html.DisplayFor(model => model.Warmup)</dd>
        <dt class="col-sm-4">  @Html.DisplayNameFor(model => model.Stretching)</dt>
        <dd class="col-sm-8">  @Html.DisplayFor(model => model.Stretching)</dd>
        <dt class="col-sm-4">  @Html.DisplayNameFor(model => model.TotalTime)</dt>
        <dd class="col-sm-8">  @Html.DisplayFor(model => model.TotalTime)</dd>
    </dl>
</div>
<div>
    <a class="btn btn-danger" asp-action="Index">Start over</a>
    <a class="btn btn-success" asp-action="Rating">Perform workout and rate it</a>
    
</div>
